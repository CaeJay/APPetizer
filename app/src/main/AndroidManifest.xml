<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caejay.appetizer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity
            android:name=".ServerHome"
            android:label="Appetizer"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ItemPage"
            android:label="Item"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".ItemList"
            android:label="Item"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TableNameInput"
            android:label="@string/title_activity_table_name_input"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Service"
            android:label="@string/title_activity_service" />
        <activity android:name=".Customize" />
        <activity android:name=".Todo" />
        <activity android:name=".OrderItemOptions"></activity>
    </application>

</manifest>